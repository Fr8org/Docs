<ul class="page-breadcrumb breadcrumb hide">
    <!-- BEGIN PAGE BREADCRUMB -->
    <li>
        <a href="#">Home</a><i class="fa fa-circle"></i>
    </li>
    <li class="active">
        Dashboard
    </li>
</ul>
<!-- END PAGE BREADCRUMB -->
<!-- BEGIN MAIN CONTENT -->

<div ng-controller="PlanListController" class="margin-top-10" id="nav-tables">
    <div class="row" style="margin:10px">
        <md-content>
            <md-tabs md-dynamic-height md-border-bottom>
                <md-tab label="My Plans">
                    <md-content class="md-padding">
                        <div id="Myfr8lines">
                            <h3>My Plans <a ng-click="addPlan()" class="btn green route-list-header-button" event-add>Add Plan</a></h3>
                            @Html.Partial("_PlanList")
                        </div>
                    </md-content>
                </md-tab>
                <md-tab label="Settings">
                    <md-content class="md-padding">
                        <div id="Myfr8lines">
                            <p>Account ID : @User.Identity.Name</p>
                        </div>
                    </md-content>
                </md-tab>
                <md-tab label="Organization" ng-if="doesOrganizationExists">
                    <md-content class="md-padding">
                        <div id="Myfr8lines">
                            @Html.Partial("Organization")
                        </div>
                    </md-content>
                </md-tab>
            </md-tabs>
        </md-content>
    </div>
</div>
<!-- END MAIN CONTENT -->
<script>
    if (window.analytics != null) {
        window.analytics.page('Plan Dashboard');
    }
</script>